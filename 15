#include <iostream>
#include <cmath>
using namespace std;
int PowerA3 (int A,int B)
{
    B=pow(A,3);
    return B;
}
int main()
{
    int N, L, C, D, E, I;
    I=0;
    D=3;
    E=5;
    C=7;
    N=2;
    L=4;
   
    cout << PowerA3 (D,I) << " ";
    cout << PowerA3 (E,I) << " ";
    cout << PowerA3 (C,I) << " ";
    cout << PowerA3 (N,I) << " ";
    cout << PowerA3 (L,I) << " ";
    return 0;
}


#include <iostream>
#include <cmath>
using namespace std;
int Sign( int X)
{
   if (X>0)
       return 1;
    else if (X==0)
        return 0;
    else return -1;
}
int main()
{
    float A, B;
    cout << "введите два числа";
    cin >> A >> B;

   
    cout << Sign( A) + Sign( B) << " ";
   
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
float RingS(float R1, float R2)
{
    float S;
    S=3.14*(pow(R1,2)-pow(R2,2));
    return S;
    
}

int main()
{
    float a, b, c, d, e, f;
    a=12;
    b=3;
    c=8;
    d=4;
    e=9;
    f=7;
    cout << RingS(a, b) << " " << RingS(c, d) << " " << RingS(e, f) << " ";
   
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
int Quarter(float x, float y)
{
  if (x>0)
      if(y>0)
          return 1;
      else return 4;
    else if (y>0)
        return 2;
    else return 3;
    
}

int main()
{
    float a, b, c, d, e, f;
    a=-12;
    b=3;
    c=-8;
    d=-4;
    e=9;
    f=7;
    cout << Quarter(a, b) << " " << Quarter(c, d) << " " << Quarter(e, f) << " ";
   
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
float Fact2(int N)
{
    int c, d;

    d=1;
    if (N%2==0) {
      c=2;
      while (c<=N) {
          d=d*c;
          c=c+2;
      }
      }
    else {
        c=1;
        while (c<=N) {
            d=d*c;
            c=c+2;
        }
    }
    return d;
}

int main()
{
    int L;
    cout << "введите число";
    cin >> L;
    cout << Fact2(L);
   
    return 0;
}
