#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N;
    cout << "введите A, B";
    cin >> A >> B;
    N=A;
    while (A<=B) {
        N=A;
        while (N>0){
            cout <<A ;
            N=N-1;
            }
        A++;
        
    }
    
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N;
    cout << "введите A, B";
    cin >> A >> B;
    N=A;
    while (A>=B) {
        A=A-B;
        
    }
    cout << A;
    return 0;
}


#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N;
    cout << "введите N";
    cin >> N;
    A=1;
    B=1;
    while (B<N) {
       
        A++;
        B=A+B;
    }
   
        
    cout << A << " " << B;
    return 0;
}
