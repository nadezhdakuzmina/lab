#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N;
    cout << "введите A, B";
    cin >> A >> B;
    N=A;
    while (A<=B) {
        N=A;
        while (N>0){
            cout <<A ;
            N=N-1;
            }
        A++;
        
    }
    
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N;
    cout << "введите A, B";
    cin >> A >> B;
    N=A;
    while (A>=B) {
        A=A-B;
        
    }
    cout << A;
    return 0;
}


#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N;
    cout << "введите N";
    cin >> N;
    A=1;
    B=1;
    while (B<N) {
       
        A++;
        B=A+B;
    }
   
        
    cout << A << " " << B;
    return 0;
}


#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A;
    float N;
    cout << "введите процент";
    cin >> N;
    N=N*0.01;
    A=0;
    B=1000;
    while (B<1100) {
       
        A++;
        B=B+B*N;
    }
   
        
    cout << A << " " << B;
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A;
    cout << "введите A, B";
    cin >> A >> B;

    while (B!=A) {
       if (A>B)
           A=A-B;
       else B=B-A;
    }
   
        
    cout << A ;
    return 0;
}

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int B, A, N, L, C;
    cout << "введите N";
    cin >> N;
    A=1;
    B=1;
    C=3;
    while (N>B) {
        L=B;
        B=A+B;
        A=L;
        C++;
    }
   
        
    cout << C ;
    return 0;
}
